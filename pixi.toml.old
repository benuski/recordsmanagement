[workspace]
authors = ["benuski <ben@amongotheritems.org>"]
channels = ["pytorch", "nvidia", "conda-forge"]
channel-priority = "disabled"
name = "rm"
platforms = ["linux-64"]
version = "0.1.0"

[system-requirements]
cuda = "12.1"

[dependencies]
python = ">=3.9,<3.13"
pytorch = "*"
torchvision = "*"
pillow = ">=10.2.0,<11.0.0"
pandas = ">=3.0.0,<4"
tabulate = ">=0.9.0,<0.10"
img2table = "*"

[pypi-dependencies]
surya-ocr = ">=0.6.0"               # Need this for table support
transformers = ">=4.41.0,<4.56.0"   # Avoid the KeyError in 4.56.0+
marker-pdf = ">=1.9.2, <2"

[tasks]

